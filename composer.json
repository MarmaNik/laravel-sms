{
  "name": "marmanik/laravel-sms",
  "description": "SMS Wrapper for Laravel using Nexmo, MessageBird, Twilio",
  "keywords": [
    "laravel",
    "sms",
    "twilio",
    "messagebird",
    "nexmo"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nikos Marmaridis",
      "email": "marmaridisn@gmail.com"
    }
  ],
  "require": {
    "php": ">=7",
    "illuminate/support": "^5.5|^6.0",
    "messagebird/php-rest-api": "^1.15",
    "nexmo/client": "^1.8",
    "twilio/sdk": "^5.34"
  },
  "autoload": {
    "psr-4": {
      "MarmaNik\\Sms\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MarmaNik\\Sms\\SmsServiceProvider"
      ],
      "aliases": {
        "Analytics": "MarmaNik\\Sms\\Facades\\Sms"
      }
    }
  },
  "minimum-stability": "stable"
}
